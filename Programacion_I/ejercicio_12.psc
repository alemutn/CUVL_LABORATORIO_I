Proceso ejercicio_12
	
	//Informar los primeros 100 números naturales y su sumatoria
	
	Definir num Como Entero;
	Definir sumNumero Como Entero;
	
	// num = 0;
	sumNumero = 0;
	
	Para num = 0 hasta 99 Con Paso 1 Hacer
		Escribir num;
		sumNumero = sumNumero + num;
	FinPara
	
	Escribir "La sumatoria del 0 al 99 es: ", sumNumero;
	
